def loader_gear_h5py(fn):
    import h5py
    from  math import sqrt
    file = h5py.File("./test_data/snapshot_0810.hdf5", "r")

    data = {}
    for pt in ['PartType0', 'PartType1', 'PartType2']:
        for field, dataset in list(file[pt].items()):
            data[pt, field] = unyt_array(dataset, 'kpc')
    return data
    
    
    
    def loader_gear_yt(fn):
    import yt
    return yt.load(fn)
    
